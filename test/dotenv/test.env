DATABASE_URL=postgres://user:password@localhost:5432/mydatabase  # URL for the PostgreSQL database
API_KEY=your_api_key_here  # API key for external services
SECRET_KEY=your_secret_key_here  # Secret key for application security
PORT=3000  # Port number for the application ;;
DEBUG=true  # Debug mode (true/false) ;;
LOG_LEVEL=info  # Logging level (e.g., info, debug, error) ;;
REDIS_URL=redis://localhost:6379  # URL for the Redis server
SMTP_SERVER=smtp.example.com  # SMTP server for sending emails
SMTP_PORT=587  # Port number for the SMTP server
SMTP_USERNAME=your_email@example.com  # Username for the SMTP server
SMTP_PASSWORD=your_email_password  # Password for the SMTP server